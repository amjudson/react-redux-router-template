{
  "name": "react-redux-router-template",
  "version": "1.0.0",
  "description": "Template for building a react/redux/router app from the ground up",
  "scripts": {
    "prestart": "babel-node server/startMessage.js",
    "start": "npm-run-all --parallel open:src",
    "watch": "webpack --progress --colors --watch",
    "open:src": "babel-node server/app.js",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "prebuild": "npm-run-all clean-dist build:html",
    "build": "babel-node server/build.js",
    "build:html": "babel-node server/buildHtml.js",
    "postbuild": "babel-node server/distServer.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "martial",
    "arts"
  ],
  "author": "Mark Judson",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.4",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-redux": "^6.0.1",
    "react-redux-router": "0.0.14",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@coreui/coreui": "^2.1.7",
    "babel-loader": "^8.0.5",
    "babel-plugin-react-transform": "^3.0.0",
    "bootstrap": "^4.3.1",
    "cheerio": "^1.0.0-rc.2",
    "clean-webpack-plugin": "^1.0.1",
    "color": "^3.1.0",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.0",
    "es6-promise": "^4.2.6",
    "eventsource-polyfill": "^0.9.6",
    "fs": "0.0.1-security",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.5.0",
    "keymirror": "^0.1.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "open": "0.0.5",
    "perfect-scrollbar": "^1.4.0",
    "popper": "^1.0.1",
    "prop-types": "^15.7.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "toastr": "^2.1.4",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-middleware": "^3.6.0",
    "webpack-dev-server": "^3.2.0",
    "webpack-hot-middleware": "^2.24.3"
  }
}
